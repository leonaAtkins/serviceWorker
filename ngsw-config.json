{
  "routing": {
    "index": "/index.html",
    "routes": {
      "/": {
        "match": "exact"
      }
    }
  },
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html",
        "/*.html",
        "/*.css",
        "/*.js",
        "/**/*.html",
        "/**/*.js",
        "/**/*.css"
      ],
      "urls": [
        "https://fonts.googleapis.com/css?family=Material+Icons",
        "https://fonts.gstatic.com/s/materialicons/v29/2fcrYFNaTjcS6g4U3t-Y5UEw0lE80llgEseQY3FEmqw.woff2"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**",
        "/assets/**/*"
      ]
    }
  }],
  "dataGroups": [{
    "name": "mapLayers",
    "urls": [
      "https://www.metoffice.gov.uk/public/data/LayerCache/OBSERVATIONS/ItemBbox/*",
      "https://a.tile.openstreetmap.org/6/*/*.png"
    ],
    "cacheConfig": {
      "maxSize": 100,
      "maxAge": "3h",
      "timeout": "1m",
      "strategy": "performance"
    }
  }]
}
